{
  "name": "ember-backstop",
  "version": "1.5.0",
  "description": "BackstopJS visual regression testing addon for Ember.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/garris/ember-backstop",
  "license": "MIT",
  "author": "garris@me.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "backstop-remote": "cd ember-backstop/; backstop remote --config=backstop.js",
    "backstop-stop": "cd ember-backstop/; backstop stop --config=backstop.js",
    "backstop-approve": "cd ember-backstop/; backstop approve --config=backstop.js",
    "backstop-report": "cd ember-backstop/; backstop openReport --config=backstop.js",
    "backstop-test": "npm run backstop-remote & ember test; npm run backstop-stop"
  },
  "dependencies": {
    "backstopjs": "garris/BackstopJS#f6a370a33d40993d7112e054e6225917e7ca5036",
    "debug": "^4.1.1",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.12.0",
    "ember-cli": "~3.28.5",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-inject-live-reload"
  }
}
