{
  "name": "ember-backstop",
  "version": "1.5.3",
  "description": "BackstopJS visual regression testing addon for Ember.",
  "keywords": [
    "ember-addon"
  ],
  "type": "commonjs",
  "repository": "https://github.com/garris/ember-backstop",
  "license": "MIT",
  "author": "garris@me.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "backstop-remote": "cd ember-backstop/; backstop remote --config=backstop.js",
    "backstop-stop": "cd ember-backstop/; backstop stop --config=backstop.js",
    "backstop-approve": "cd ember-backstop/; backstop approve --config=backstop.js",
    "backstop-report": "cd ember-backstop/; backstop openReport --config=backstop.js",
    "backstop-test": "npm run backstop-remote & ember test; npm run backstop-stop"
  },
  "dependencies": {
    "backstopjs": "garris/BackstopJS#d4536fa376a32283b3eb7678cbe281ceed3b2983",
    "debug": "^4.1.1",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^5.7.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "^5.4.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^8.1.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "@ember/jquery": "^2.0.0"
  },
  "engines": {
    "node": ">= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-inject-live-reload"
  }
}
